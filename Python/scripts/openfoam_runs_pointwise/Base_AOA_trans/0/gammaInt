FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      gammaInt;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    farfield
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           uniform 1;
    }
    side
    {
        type            empty;
    }
    airfoil
    {
        type            zeroGradient;
    }
}


