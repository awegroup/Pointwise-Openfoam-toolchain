FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];


internalField   uniform 299.9999999999999;

boundaryField
{
    airfoil
    {
        type            omegaWallFunction;
        value           uniform 299.9999999999999;
    }
    farfield
    {
        type            inletOutlet;
        inletValue      uniform 299.9999999999999;
        value           uniform 299.9999999999999;
    }
    side
    {
        type            empty;
    }
}


