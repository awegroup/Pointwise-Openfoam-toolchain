{
    airfoil
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    farfield
    {
        type            inletOutlet;
        inletValue      uniform (0.984807753012208 0.1736481776669303 0);
        value           uniform (0.984807753012208 0.1736481776669303 0);
    }
    side
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (0.984807753012208 0.1736481776669303 0);
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform (0.984807753012208 0.1736481776669303 0);
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform (0.984807753012208 0.1736481776669303 0);
    }
