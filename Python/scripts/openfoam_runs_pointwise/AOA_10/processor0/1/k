{
    airfoil
    {
        type            fixedValue;
        value           uniform 0;
    }
    farfield
    {
        type            inletOutlet;
        inletValue      uniform 0.0006000000000000001;
        value           uniform 0.0006000000000000001;
    }
    side
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 0.0006000000000000001;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 0.0006000000000000001;
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform 0.0006000000000000001;
    }
}


