FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    airfoil
    {
        type            zeroGradient;
    }
    farfield
    {
        type            outletInlet;
        outletValue     uniform 0;
        value           uniform 0;
    }
    side
    {
        type            empty;
    }
    procBoundary3to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 0;
    }
}


